<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyContacts.Views.ContactsPage"
             Title="Contacts">
    <ListView x:Name="contactsList" BackgroundColor="Transparent" ItemSelected="contactsList_ItemSelected"
              ItemTapped="contactsList_ItemTapped">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding name}" Detail="{Binding number}"></TextCell>

            </DataTemplate>

        </ListView.ItemTemplate>

    </ListView>
</ContentPage>
