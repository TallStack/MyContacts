    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MyContacts"
    xmlns:views="clr-namespace:MyContacts.Views"
    xmlns:mvvmviews="clr-namespace:MyContacts.Views_MVVM"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate mvvmviews:Contacts_MVVM_Page}"
        Route="ContatsPage"/>

</Shell>

